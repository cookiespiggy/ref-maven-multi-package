<assembly>
    <id>all</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <useProjectArtifact>false</useProjectArtifact>
            <outputDirectory>/</outputDirectory>
            <unpack>true</unpack>
            <scope>compile</scope>
            <includes>
                <include>org.example:pelican-analysis</include>
                <include>org.example:pelican-utils</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <useProjectArtifact>true</useProjectArtifact>
            <outputDirectory>/</outputDirectory>
            <unpack>true</unpack>
            <includes>
                <include>org.example:ndds-ncas</include>
            </includes>
        </dependencySet>
    </dependencySets>

</assembly>

